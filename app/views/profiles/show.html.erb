<p id="notice"><%= notice %></p>

  <p>
  <strong>User:</strong>
  <%= @profile.user_id %>
</p>
<p>
  <strong>Current User:</strong>
  <%= current_user.id %>
</p>

<p>
  <strong>Name:</strong>
  <%= @profile.first_name + " " + @profile.last_name %>
</p>

<p>
  <strong>Case Number:</strong>
  <%= @profile.case_number %>
</p>
<p>
  <strong>User Name:</strong>
  <%= current_user.email %>
</p>

<p>
  <strong>Birth date:</strong>
  <%= @profile.birth_date %>
</p>
<p>
  <strong>Gender:</strong>
  <%= @profile.gender %>
</p>
<p>
  <strong>Height:</strong>
  <%= @profile.height %>
</p>
<p>
  <strong>Weight:</strong>
  <%= @profile.weight %>
</p>
<p>
  <strong>DL Number:</strong>
  <%= @profile.dl_number %>
</p>
<p>
  <strong>DL State:</strong>
  <%= @profile.dl_state %>
</p>
<p>
  <strong>Ssn:</strong>
  <%= @profile.ssn %>
</p>

<p>
  <strong>Home phone:</strong>
  <%= @profile.home_phone %>
</p>

<p>
  <strong>Work phone:</strong>
  <%= @profile.work_phone %>
</p>

<p>
  <strong>Cell phone:</strong>
  <%= @profile.cell_phone %>
</p>

<p>
  <strong>Address:</strong><br>
  <% if @profile.add_line1.present? %>
    <%= @profile.add_line1  %><br>
  <% end %>
  <% if @profile.add_line2.present? %>
    <%= @profile.add_line2  %><br>
  <% end %>
  <% if @profile.add_city.present? %>
    <%= @profile.add_city  %>,
  <% end %>
  <% if @profile.add_state.present? %>
    <%= @profile.add_state  %>
  <% end %>
  <% if @profile.add_zip.present? %>
    <%= @profile.add_zip  %><br>
  <% end %>
</p>


<%= link_to 'Edit', edit_profile_path(@profile) %>

