<p id="notice"><%= notice %></p>

<h1>Listing User Details</h1>

<div class="media">
    <div class="media-body">
      <table class="table table-hover">
        <thead>
          <tr>
            <th>Name</th>
            <th>Date of birth</th>
            <th>Party type</th>
            <th>Case</th>
            <th>Phone</th>
            <th>Address</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @user_details.each do |user_detail| %>
            <tr>
              <td><%= user_detail.name %></td>
              <td><%= user_detail.date_of_birth %></td>
              <td><%= user_detail.party_type %></td>
              <td><%= user_detail.case_id %></td>
              <td><%= user_detail.phone %></td>
              <td><%= user_detail.address %></td>
              <td><%= link_to 'Show', user_detail, class: 'btn btn-mini'%></td>
              <td><%= link_to 'Edit', edit_user_detail_path(user_detail), class: 'btn btn-mini btn-success' %></td>
              <td><%= link_to 'Destroy', user_detail, method: :delete, class: 'btn btn-mini btn-danger', data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
<br>

<%= link_to 'New User detail', new_user_detail_path %>
